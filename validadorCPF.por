programa {
  funcao inicio() {
    inteiro p1, p2, p3, p4
    
    escreva("Digite os três primeiros digitos do CPF: ")
    leia(p1)
    limpa()

    escreva("Digite os outros três: ")
    leia(p2)
    limpa()

    escreva("Digite os outros três: ")
    leia(p3)
    limpa()

    escreva("Digite os dois últimos: ")
    leia(p4)
    limpa()

    escreva("CPF: ", p1, ".", p2, ".", p3, "-", p4)

  }
}
